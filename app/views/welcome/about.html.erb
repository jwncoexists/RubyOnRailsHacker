
<!-- Home Carousel -->

<section id="home-carousel" class="home-section">
    <div class="wrap-pattern">
        <div class="container">
            <div id="carousel-home" class="carousel slide" data-ride="carousel"  data-interval="3000">
                <!-- Indicators -->

                <div class="carousel-inner">
                    <div class="item active">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="slogan home-section">
                                    <p></p>
                                    <p id="aboutSlogan" class="lead">Turning Ideas into Web Applications!</p>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <%= image_tag("banner.png", class: "img-responsive", alt: "Image") %>
                            </div>
                        </div>
                    </div>
                </div> <!-- carousel-inner -->
            </div> <!-- carousel-home -->
        </div> <!-- container -->
    </div> <!-- wrap-pattern -->
</section> <!--  home-carousel -->



<!-- home-slogan -->
<section class="slogan home-section">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <p class="lead"><span>Using </span>Ruby on Rails <span>OOD </span>JQuery <span>BDD </span>AngularJS <span>Javascript  </span>JQuery <span>Relational DBs </span>Resque <span>TDD </span>HTML <span>CSS</span>.</p>
            </div>
        </div>
    </div> <!-- container -->
</section> <!-- home-slogan -->


<!-- Home-about -->
<section id="home-about" class="home-section">
    <div class="container">
        <div class="row">
            <div class="col-md-6 home-about-text">
                <h1 class="section-title">About</h1>
                <p>Jennifer Nelson is a freelance web developer living in San Diego, California. She has extensive experience with a variety of frameworks and languages and has worked for startups as well as large corporations. </p>

                <p>She is passionate and driven to create quality software applications, on schedule, and within budget. Jennifer believes in test driven development, and excels at taking ideas from concept through to implementation.</p>

            </div> <!-- home-about-test -->

            <div class="col-md-6 home-about-features">
              <%= image_tag("JN.png", class: "img-responsive", alt: "Jennifer Nelson") %>
            </div> <!-- home-about-features -->
        </div> <!-- row -->
    </div> <!-- cointainer -->
</section> <!-- home-about -->

