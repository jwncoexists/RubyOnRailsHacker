        </div>
      </div>

      <div class="col-md-3 col-sm-4 wow bounceInRight">
        <div class="stat">
          <div class="stat-icon">
            <h2><i class="fa fa-refresh hidden-xs"></i><span class="timer" data-to="2"></span></h2>
          </div>
          <h3>hours hot yoga</h3>
        </div>
      </div>

    </div><!-- .row -->
  </div><!-- .container -->
</section>
<!-- Callout section end -->

